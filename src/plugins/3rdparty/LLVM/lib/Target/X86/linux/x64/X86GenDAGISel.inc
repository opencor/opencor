  case 45: return (TM.getCodeModel() == CodeModel::Small);
  case 46: return (TM.getCodeModel() == CodeModel::Kernel);
  case 47: return (Subtarget->hasRDRAND());
  case 48: return (Subtarget->hasRDSEED());
  case 49: return (Subtarget->hasRTM() || Subtarget->hasHLE());
  case 50: return (Subtarget->hasSSE1() && !Subtarget->hasAVX()) && (OptForSize);
  case 51: return (Subtarget->has3DNow() || Subtarget->hasPRFCHW());
  case 52: return (!Subtarget->is64Bit()) && (TM.getRelocationModel() != Reloc::PIC_);
